

package com.o2.osiris.persistence;

/**
 The PersistenceException is only thrown by the Persistence constructor. 
 
 import capgemini.oss.common.utils.*
 extends OSSException
 */
public class PersistenceException extends Exception
{
   /**
    default constructor.
    Calls the superclass (OSSException) constructor.
    */
   public PersistenceException() 
   {
		super();
   }
   
   /**
    Calls the superclass (OSSException) constructor and passes the message to it.
    */
   public PersistenceException(String message) 
   {
		super(message);
   }
   
   /**
    Calls the superclass (OSSException) constructor and passes the Throwable object to it.
    */
   public PersistenceException(Throwable cause) 
   {
		super(cause);
   }
   
   public PersistenceException(String message, Throwable cause) 
   {
		super(message, cause);
   }   

   public PersistenceException(Throwable cause, String message) 
   {
		super(message, cause);
   }   

}
